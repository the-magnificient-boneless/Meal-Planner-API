# Puerto del servidor Express
PORT=8000

# API Key generada desde MongoDB Atlas App Services (Data API)
MONGODB_DATA_API_KEY=your_mongodb_data_api_key_here

# URL base de tu Data API (reemplaza con tu propia app endpoint)
DATA_API_URL=https://us-east-1.aws.data.mongodb-api.com/app/your-app-id/endpoint/data/v1

# Nombre del cluster configurado en Atlas (por defecto suele ser Cluster0)
DATA_SOURCE=Cluster0

# Nombre de la base de datos donde est√°n las colecciones (ej: geo, appdata, etc.)
DB_NAME=your_database_name
